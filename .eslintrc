{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "react-native"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "extends" : [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ],
  "rules" : {
    "arrow-body-style" : "warn",
    "indent": ["error", "tab"],
    "quotes": ["error", "double"],
    "react/prop-types": 0,
    "import/no-extraneous-dependencies": 0,
    "no-tabs": 0
  }
}
